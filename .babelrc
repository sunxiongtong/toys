{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "modules": false
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": 2,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ],
        "@babel/plugin-proposal-class-properties",
        [
            "import",
            {
              "libraryName": "antd-mobile",
              // "libraryDirectory": "es",
              "style": "css"
            }
          ]
    ]
}